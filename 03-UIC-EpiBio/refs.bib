	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/cgf.13079},
	keywords = {eye tracking, Human-centred computing: Visualization—Visualization techniques Human-centred computing: Visualization—Visualization design and evaluation methods, survey, visualization},
	file = {Full Text PDF:/home/susan/Nextcloud/Zotero/storage/EHK6S8GQ/Blascheck et al. - 2017 - Visualization of Eye Tracking Data A Taxonomy and.pdf:application/pdf;Snapshot:/home/susan/Nextcloud/Zotero/storage/5S858Z92/cgf.html:text/html},
