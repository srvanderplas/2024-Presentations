	note = {Publisher: American Psychological Association},
	keywords = {Adolescent, Attention, Cognition, complexity, Comprehension, Experience Level, expertise, Female, Humans, Judgment, Male, metacognition, Metacognition, Photic Stimulation, Reaction Time, realism, Realism (Philosophy), Task Performance and Analysis, visual display, Visual Displays, Visual Perception, Young Adult},
	file = {Hegarty et al. - 2012 - Choosing and using geospatial displays Effects of.pdf:/home/susan/Nextcloud/Zotero/storage/TPQY5QN5/Hegarty et al. - 2012 - Choosing and using geospatial displays Effects of.pdf:application/pdf},
